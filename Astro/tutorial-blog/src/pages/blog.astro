---
import BaseLayout from "../layouts/BaseLayout.astro";
import "../styles/global.css";
const pageTitle = "Mi blog de astro aprendizaje";
const allPost = Object.values(import.meta.glob('./post/*.md', {eager: true}))
---

<BaseLayout pageTitle={pageTitle}>
  <p>Aqui es donde publiocare mi viaje de aprendizaje Astro</p>

  <ul>
    {allPost.map((post: any) => (
        <li><a href={post.url}>{post.frontmatter.title}</a></li>
    ))}
  </ul>
</BaseLayout>
